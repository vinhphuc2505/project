<odoo>
    <data noupdate="1">
        <!-- Sequence cho thành viên -->
        <record id="seq_clb_member" model="ir.sequence">
            <field name="name">Mã thành viên</field>
            <field name="code">clb.member</field>
            <field name="prefix">MEM/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence cho sự kiện -->
        <record id="seq_event_management" model="ir.sequence">
            <field name="name">Mã sự kiện</field>
            <field name="code">event.management</field>
            <field name="prefix">EVT/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_clb_management" model="ir.sequence">
            <field name="name">CLB Management Sequence</field>
            <field name="code">clb.management</field>
            <field name="prefix">CLB-</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>


        <!-- Sequence for Event Registration -->
        <record id="seq_event_registration" model="ir.sequence">
            <field name="name">Mã đăng ký sự kiện</field>
            <field name="code">event.registration</field>
            <field name="prefix">REG/</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>